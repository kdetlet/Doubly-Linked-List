
#ifndef _INT_DL_LIST_H_
#define _INT_DL_LIST_H_

class intDLList
{
	friend class intDLLNode;
public:
	intDLList();
	~intDLList();
	bool isEmpty();
	void addToHead(int i);
	void addToTail(int i);
	int deleteFromHead();
	int deleteFromTail();
	void deleteNode(int i);
	bool isInList(int i) const;
	void addSorted(int i);
	void printList();
	
	

protected:

private:
	intDLLNode *head;
	intDLLNode *tail;

};

#endif // _INT_DL_LIST_H_

